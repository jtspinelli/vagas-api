{
  "name": "api-vagas",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --silent --noStackTrace --runInBand",
    "test:unit": "jest -c jest.unit.config.ts --silent --noStackTrace --runInBand",
    "test:unit:verbose": "jest -c jest.unit.config.ts --verbose --runInBand --detectOpenHandles",
    "test:unit:coverage": "jest -c jest.unit.config.ts --silent --noStackTrace --runInBand --coverage",
    "test:unit:watch": "jest -c jest.unit.config.ts --silent --noStackTrace --runInBand --watchAll",
	"test:integration": "cross-env NODE_ENV=test jest -c jest.integration.config.ts --silent --noStackTrace --runInBand",
    "test:integration:verbose": "cross-env NODE_ENV=test jest -c jest.integration.config.ts --verbose --runInBand --detectOpenHandles --forceExit",
    "test:integration:coverage": "cross-env NODE_ENV=test jest -c jest.integration.config.ts --silent --noStackTrace --runInBand --coverage",
    "test:integration:watch": "cross-env NODE_ENV=test jest -c jest.integration.config.ts --silent --noStackTrace --runInBand --watchAll",
    "build": "npx tsc",
    "start": "node out/main/index.js",
    "dev": "nodemon src/main/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:g": "npm run typeorm migration:generate -- src/app/shared/database/migrations/GenerateDb -d src/main/config/dataSource.ts",
    "migration:g:test": "cross-env NODE_ENV=test npm run typeorm migration:generate -- src/app/shared/database/migrations/GenerateDb -d src/main/config/dataSource.ts",
    "migration:r": "npm run typeorm -- migration:run -d src/main/config/dataSource.ts",
    "migration:r:test": "cross-env NODE_ENV=test npm run typeorm -- migration:run -d src/main/config/dataSource.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.3",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@automapper/classes": "^8.7.7",
    "@automapper/core": "^8.7.7",
    "@types/jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.16",
    "uuid": "^9.0.0"
  }
}
